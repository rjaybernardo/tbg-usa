        "question_4yknba": {
          "type": "question",
          "settings": {
            "title": "How many come in a set?",
            "answer": "<p>There are three pots in every Slugg set.<\/p>"
          }
        },
        "question_RwraJq": {
          "type": "question",
          "settings": {
            "title": "Are they cheaper?",
            "answer": "<p>Yes! Because Sluggs are sold as three pots in a box, the shipping is cheaper and they require less handling in our warehouse. Sluggs work out to be roughly 50% cheaper than purchasing the same pots individually.<\/p>"
          }
        },
        "question_UrxYHN": {
          "type": "question",
          "settings": {
            "title": "Can I mix and max the colours?",
            "answer": "<p>No. Sluggs are pre-packaged so we are unable to mix and match colours or styles.<\/p>"
          }
        },
        "question_cA486P": {
