    "dingo_faq_8_xgNnMW": {
      "type": "Dingo-FAQ-8",
      "blocks": {
        "faq_item_xFTQNw": {
          "type": "faq_item",
          "settings": {
            "faq_item_question": "<p><strong>What's your return policy?<\/strong><\/p>",
            "faq_item_answer": "<p>While we will try our best to make changes to any order, orders cannot be changed once they have been picked and\/or packed. We do not offer returns for a change of mind. Please choose wisely.<\/p>"
          }
        },
        "faq_item_R77jY9": {
          "type": "faq_item",
          "settings": {
            "faq_item_question": "<p><strong>What are the pots made of?<\/strong><\/p>",
            "faq_item_answer": "<p>Our Willow pots are made of a blend of poly resin and fibreglass making it extremely strong, waterproof and lightweight.  Our Bad Names range is made from wood-fired clay. Our pots can be used indoors and outdoors.<\/p>"
          }
        },
        "faq_item_RQrNEM": {
          "type": "faq_item",
          "settings": {
            "faq_item_question": "<p><strong>Is it safe and convenient to order pots online?<\/strong><\/p>",
            "faq_item_answer": "<p>Ordering online will save you a trip to our showroom and we’ll organise the delivery for you. Why go through all the hassle of renting a trailer or a ute, when you can get your beautiful new pots dropped at your front door! We take great care in wrapping and packaging our pots so they get to you in the best condition.<\/p>"
          }
        },
        "faq_item_ptnCtc": {
          "type": "faq_item",
          "settings": {
            "faq_item_question": "<p><strong>Are the pots handmade?<\/strong><\/p>",
            "faq_item_answer": "<p>Yes! All of our pots are handmade. Products may have slight imperfections and colour variations.<\/p>"
          }
        }
      },
