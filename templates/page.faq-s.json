    "dingo_faq_8_UqzEd9": {
      "type": "Dingo-FAQ-8",
      "blocks": {
        "faq_item_k4TPA9": {
          "type": "faq_item",
          "settings": {
            "faq_item_question": "<p><strong>What happens if my pots arrive damaged?<\/strong><\/p>",
            "faq_item_answer": "<p>We’ve 100% got your back.  If your pots arrive damaged, simply get in touch with us within 24 hours and we’ll either refund you or send you a replacement.<\/p>"
          }
        },
        "faq_item_xVdRnp": {
          "type": "faq_item",
          "settings": {
            "faq_item_question": "<p><strong>Does the delivery include unpacking, product placement, and rubbish removal?<\/strong><\/p>",
            "faq_item_answer": "<p>No, The Balcony Garden nor any third-party courier service is responsible for unpacking, product placement or rubbish removal.<\/p>"
          }
        },
        "faq_item_QXeWWT": {
          "type": "faq_item",
          "settings": {
            "faq_item_question": "<p><strong>How is my order packaged?<\/strong><\/p>",
            "faq_item_answer": "<p>Depending on the size of your order it will arrive in boxes or on a pallet. <\/p>"
          }
        },
        "faq_item_pkA9Db": {
          "type": "faq_item",
          "settings": {
            "faq_item_question": "<p><strong>Where do you ship to?<\/strong><\/p>",
            "faq_item_answer": "<p>We ship to all states in the US, excluding Alaska and Hawaii.<\/p>"
          }
        },
        "faq_item_3VBAmK": {
          "type": "faq_item",
          "disabled": true,
