    "dingo_faq_8_xgNnMW": {
      "type": "Dingo-FAQ-8",
      "blocks": {
        "faq_item_xFTQNw": {
          "type": "faq_item",
          "settings": {
            "faq_item_question": "<p><strong>What's your return policy?<\/strong><\/p>",
            "faq_item_answer": "<p>While we will try our best to make changes to any order, orders cannot be changed once they have been picked and\/or packed. We do not offer returns for a change of mind. Please choose wisely.<\/p>"
          }
        },
        "faq_item_R77jY9": {
          "type": "faq_item",
          "settings": {
            "faq_item_question": "<p><strong>What are the pots made of?<\/strong><\/p>",
            "faq_item_answer": "<p>Our Willow and Slugg Ranges are made from a strong, waterproof, and lightweight polyresin-fibreglass blend, while the Bad Names Range features unique, wood-fired clay pots. All are perfect for indoor and outdoor use.<\/p>"
          }
        },
        "faq_item_RQrNEM": {
          "type": "faq_item",
          "settings": {
            "faq_item_question": "<p><strong>Is it safe and convenient to order pots online?<\/strong><\/p>",
            "faq_item_answer": "<p>Ordering online is easy and secure. We take great care in packaging our pots to ensure they arrive safely and in perfect condition.<\/p>"
          }
        },
        "faq_item_ptnCtc": {
          "type": "faq_item",
          "settings": {
            "faq_item_question": "<p><strong>Are the pots handmade?<\/strong><\/p>",
            "faq_item_answer": "<p>Yes! All of our pots are handmade. Products may have slight imperfections and colour variations.<\/p>"
          }
        },
        "faq_item_4NPXK7": {
          "type": "faq_item",
          "settings": {
            "faq_item_question": "<p>​<strong>Are there any benefits for first-time customers?<\/strong><\/p>",
            "faq_item_answer": "<p>First-time customers enjoy a 15% discount on their initial order. Simply sign up to access this exclusive offer.<\/p>"
          }
        }
      },
